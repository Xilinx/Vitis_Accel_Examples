{
    "name": "Array Block and Cyclic Partitioning (OpenCL Kernel)",
    "description": [
        "This example shows how to use array block and cyclic partitioning to improve performance of a kernel"
    ], 
    "flow": "vitis",
    "keywords": [
        "xcl_array_partition", 
        "cyclic", 
        "block"
    ], 
    "key_concepts": [
        "Kernel Optimization", 
        "Array Partitioning", 
        "Block Partition", 
        "Cyclic Partition"
    ], 
    "platform_blacklist": [
        "zc702",
        "nodma"
    ],
    "os": [
        "Linux"
    ], 
    "runtime": [
        "OpenCL"
    ], 
    "host": {
        "host_exe": "cl_partition_cyclicblock",
        "compiler": {
            "sources": [
                "REPO_DIR/common/includes/xcl2/xcl2.cpp",
                "./src/host.cpp"
            ], 
            "includepaths": [
                "REPO_DIR/common/includes/xcl2"
            ]
        }
    }, 
    "containers": [
        {
            "accelerators": [
                {
                    "name": "matmul_naive", 
                    "location": "src/matmul.cl"
                }, 
                {
                    "name": "matmul_partition", 
                    "location": "src/matmul.cl"
                }
            ], 
            "name": "matmul"
        }
    ], 
    "launch": [
        {
            "cmd_args": "BUILD/matmul.xclbin", 
            "name": "generic launch for all flows"
        }
    ], 
    "output_files": [
        "output.png"
    ], 
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
    ], 
    "testinfo": {
        "level_up": [
            {
                "hw": "1"
            }
        ],
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": 32768,
                "max_time_min": 300
            }
        ],
        "targets": [
            "vitis_sw_emu",
            "vitis_hw_emu",
            "vitis_hw"
        ],
        "category": "canary"
    }
}
