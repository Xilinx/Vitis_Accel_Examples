{
    "name": "Loop Reordering (OpenCL Kernel)", 
    "description": [
        "This is a simple example of matrix multiplication (Row x Col) to demonstrate how to achieve better pipeline II factor by loop reordering."
    ],
    "flow": "vitis",
    "keywords": [
        "xcl_pipeline_loop", 
        "xcl_array_partition"
    ], 
    "key_concepts": [
        "Kernel Optimization", 
        "Loop reorder to improve II"
    ],
    "platform_blacklist": [
        "nodma"
    ], 
    "os": [
        "Linux"
    ], 
    "runtime": [
        "OpenCL"
    ], 
    "host": {
        "host_exe": "cl_loop_reorder",
        "compiler": {
            "sources": [
                "REPO_DIR/common/includes/xcl2/xcl2.cpp",
                "./src/host.cpp"
            ], 
            "includepaths": [
                "REPO_DIR/common/includes/xcl2"
            ]
        }
    }, 
    "containers": [
        {
            "accelerators": [
                {
                    "name": "mmult", 
                    "location": "src/mmult.cl"
                }
            ], 
            "name": "mmult"
        }
    ],
    "launch": [
        {
            "cmd_args": "BUILD/mmult.xclbin", 
            "name": "generic launch for all flows"
        }
    ], 
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": 32768,
                "max_time_min": 300
            }
        ],
        "targets": [
            "vitis_sw_emu",
            "vitis_hw_emu",
            "vitis_hw"
        ],
        "category": "canary"
    }
}
