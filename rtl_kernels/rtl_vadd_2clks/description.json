{
    "name": "Vadd 2 Kernel Clocks (RTL Kernel)", 
    "description": [
        "This example shows vector addition with 2 kernel clocks using RTL Kernel."
    ], 
    "flow": "vitis",
    "keywords": [
	"kernel_frequency"
    ], 
    "key_concepts": [
        "RTL Kernel", 
        "Multiple Kernel Clocks"
    ],
    "platform_type": "pcie", 
    "platform_blacklist": [
        "zc",
        "vck",
        "nodma"
    ], 
    "targets": [
        "hw", 
        "hw_emu"
    ], 
    "os": [
        "Linux"
    ], 
    "runtime": [
        "OpenCL"
    ], 
    "gui": false, 
    "host": {
        "host_exe": "rtl_vadd_2clks",
        "compiler": {
            "sources": [
                "REPO_DIR/common/includes/xcl2/xcl2.cpp", 
                "src/host.cpp"
            ], 
            "includepaths": [
                "REPO_DIR/common/includes/xcl2"
            ]
        }
    }, 
    "containers": [
        {
            "accelerators": [
                {
                    "kernel_type": "RTL", 
                    "name": "vadd"
                }
            ], 
            "name": "vadd",
	    "ldclflags": "--config PROJECT/vadd.cfg"
        }
    ], 
    "launch": [
        {
            "cmd_args": "BUILD/vadd.xclbin", 
            "name": "generic launch for all flows"
        }
    ], 
    "config_make": "config.mk", 
    "output_files": [
        "tmp_kernel_pack*",
        "packaged_kernel*"
    ],
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
    ], 
    "revision": [
        {
            "date": "JAN2017", 
            "version": "1.0", 
            "description": "Initial Xilinx Release"
        }
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": 32768,
                "max_time_min": 300
            }
        ],
        "targets": [
            "vitis_hw_emu",
            "vitis_hw"
        ],
        "category": "canary"
    } 
}
