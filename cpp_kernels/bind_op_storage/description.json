{
    "name": "BIND OP and STORAGE (HLS C/C++ Kernel)", 
    "description": [
        "This is simple example of vector addition to describe how to use BIND OP and STORAGE for better implementation style."
    ],
    "flow": "vitis",
    "keywords": [
        "BIND_OP", 
        "BIND_STORAGE",
        "impl", 
        "op",
        "type", 
        "latency" 
    ], 
    "key_concepts": [
        "BIND OP",
        "BIND STORAGE" 
    ], 
    "platform_blacklist": [
        "zcu102",
        "zc706",
        "nodma"
     ],
    "os": [
        "Linux"
    ], 
    "runtime": [
        "OpenCL"
    ], 
    "host": {
        "host_exe": "bind_op_storage",
        "compiler": {
            "sources": [
                "REPO_DIR/common/includes/xcl2/xcl2.cpp",
                "./src/host.cpp"
            ], 
            "includepaths": [
                "REPO_DIR/common/includes/xcl2"
            ]
        }
    }, 
    "containers": [
        {
            "accelerators": [
                {
                    "name": "vadd", 
                    "location": "src/vadd.cpp"
                }
            ], 
            "name": "vadd"
        }
    ],
    "launch": [
        {
            "cmd_args": "BUILD/vadd.xclbin", 
            "name": "generic launch for all flows"
        }
    ], 
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": 32768,
                "max_time_min": 300
            }
        ],
        "targets": [
            "vitis_sw_emu",
            "vitis_hw_emu",
            "vitis_hw"
        ],
        "category": "canary"
    }
}
